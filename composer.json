{
    "name": "dreadfulcode/eloquent-model-generator",
    "description": "Eloquent Model Generator",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dreadfulcode\\EloquentModelGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dreadfulcode\\Tests\\Integration\\": "tests/integration"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.30",
        "illuminate/support": "^10.30",
        "illuminate/contracts": "^10.30",
        "illuminate/config": "^10.30", 
        "illuminate/console": "^10.30",
        "doctrine/dbal": "^2.0 || ^3.1.4",
        "krlove/code-generator": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.37"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dreadfulcode\\EloquentModelGenerator\\Provider\\GeneratorServiceProvider"
            ]
        }
    }
}
