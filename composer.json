{
    "name": "dreadfulcode/eloquent-model-generator",
    "description": "Eloquent Model Generator",
    "version": "2.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dreadfulcode\\EloquentModelGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dreadfulcode\\Tests\\Integration\\": "tests/integration"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/database": "*",
        "illuminate/support": "*",
        "illuminate/config": "*",
        "illuminate/console": "*",
        "doctrine/dbal": "*",
        "krlove/code-generator": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.37"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dreadfulcode\\EloquentModelGenerator\\Provider\\GeneratorServiceProvider"
            ]
        }
    }
}
